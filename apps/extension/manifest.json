{
    "manifest_version": 3,
    "name": "ArchiveStream One-Click",
    "version": "1.0.0",
    "description": "Capture any web page instantly to your ArchiveStream instance.",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon128.png"
    },
    "icons": {
        "128": "icon128.png"
    }
}